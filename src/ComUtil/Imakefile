Obj(funcptrs)
Obj(lexscan)
Obj(mblock)
Obj(optable)
Obj(parser)
Obj(scanner)
Obj(symbols)
Obj(txtutil)
Obj(types)
Obj(util)
Obj(xdll)

IncludeDependencies()

#else

InstallXDefaultAs(comterp.err,$(LIBALLDIR),comterp.err)

